    "author": "Aseem Kishore <aseem.kishore@gmail.com>",
    "contributors": [
        "Daniel Gasienica <daniel@gasienica.ch>",
        "Sergio Haro <sergio.haro.jr@gmail.com>"
    ],
    "main": "./lib",
    "dependencies": {
